Gem::Specification.new do |s|
  s.name               = "ow-nuxeo"
  s.executables << "ow-nuxeo"
  s.summary = %q{A script for Nuxeo projects based on the Open Wide skeleton}
  s.description = %q{Eases common tasks with Nuxeo projects, from building to launching. Mainly targeted to Open Wide projects, but could fit any Maven project hierarchy using a Nuxeo Marketplace package for distribution.}
  
  s.version            = "0.1.0"
  s.date = %q{2013-07-12}
  
  s.authors = ["Marwane Kalam-Alami"]
  s.email = %q{marwane.kalam-alami@openwide.fr}
  
  s.files = ["bin/ow-nuxeo"]
  s.require_paths = ["bin"]
  s.homepage = %q{http://rubygems.org/gems/ow-nuxeo}
end

